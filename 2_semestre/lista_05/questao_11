função Soma_Valores_Seq(nome: string): inteiro
var
    f, f2: sequencia de string
    s: string
    num, soma: inteiro
inicio
    se abre(f, nome) entao
        soma <- 0
        enquanto nao fds(f) faça
            leia(f, s)
            se abre(f2, s) entao
                enquanto nao fds(f2) faça
                    leia(f2, num)
                    soma <- soma + num
                fim enquanto
                fecha(f2)
            fim se
        fim enquanto
        fecha(f)
        retorne soma
    fim se
    retorne -9999999
fim função